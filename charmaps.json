  "name": "gb18030-front-end",
  "description": "GB18030 to Unicode.",
  "version": "",
  "type": "front-end",
  "path": "charmaps/front-gb18030-little-endian.map",
  "rules": [
    {
      "byte": 1,
      "condition": ["0x00~0x7F"]
    }, {
      "byte": 1,
      "condition": ["0x80"]
    }, {
      "byte": 1,
      "condition": ["0xFF"]
    }, {
      "byte": 2,
      "condition": ["0x81~0xFE", "0x40~0x7E"]
    }
  ]
}]